# Environment configuration
# Copy this file to .env and fill in your API keys

# === REQUIRED API KEYS ===
# OpenAI API Key (required for LLM agents)
# Get from: https://platform.openai.com/api-keys
OPENAI_API_KEY=

# Google AI API Key (required for medical imaging analysis)
# Get from: https://aistudio.google.com/apikey
GOOGLE_API_KEY=

# === OPTIONAL LLM CONFIGURATION ===
OPENAI_MODEL=gpt-4o-mini
GEMINI_MODEL=gemini-1.5-pro-latest
LOG_LEVEL=INFO

# === OPTIONAL EXTERNAL INTEGRATIONS ===
# Medical terminology and drug databases (stubs provided)
UMLS_API_KEY=
RXNORM_API_KEY=
DRUGBANK_API_KEY=

# Healthcare system integrations (stubs provided)
FHIR_BASE_URL=
SCHEDULER_BASE_URL=

# Clinical guidelines API (optional)
GUIDELINES_API_URL=
GUIDELINES_API_KEY=

# === APP CONFIGURATION ===
APP_ENV=development
CREWAI_TRACING_ENABLED=false
